.sthoverbuttons-pos-left { left: 0px; }

.sthoverbuttons-pos-right { right: 0px; }

#sthoverbuttons #sthoverbuttons-color { height: 100%; position: absolute; width: 100%; z-index: 985000; }

#sthoverbuttons #sthoverbuttons-top { height: 5px; width: 100%; }

#sthoverbuttons #sthoverbuttons-bottom { height: 10px; width: 100%; }

#sthoverbuttons #sthoverbuttons-shade { height: 100%; width: 100%; }

#sthoverbuttons .sthoverbuttons-top-l { background: url("../images/mini_Top_Cap.png") 0px 0px no-repeat scroll transparent; }

#sthoverbuttons .sthoverbuttons-bottom-l { background: url("../images/mini_Bottom_Cap.png") 0px 0px no-repeat scroll transparent; }

#sthoverbuttons .sthoverbuttons-shade-l { background: url("../images/mini_BG.png") 0px 0px repeat-y scroll transparent; }

#sthoverbuttons .sthoverbuttons-top-r { background: url("../images/mini_Top_Cap_R.png") 0px 0px no-repeat scroll transparent; }

#sthoverbuttons .sthoverbuttons-bottom-r { background: url("../images/mini_Bottom_Cap_R.png") 0px 0px no-repeat scroll transparent; }

#sthoverbuttons .sthoverbuttons-shade-r { background: url("../images/mini_BG_R.png") 0px 0px repeat-y scroll transparent; }

#sthoverbuttons #sthoverbuttons-background { position: absolute; top: 0px; width: 100%; z-index: 990000; height: 100%; }

#sthoverbuttons .sthoverbuttons-background-l { left: 7px; }

#sthoverbuttons .sthoverbuttons-background-r { right: -3px; }

